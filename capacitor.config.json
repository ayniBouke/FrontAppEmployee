{
    "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": ["google.com", "phone"],
      "languageCode": "en",
      "nativeAuth": false,
      "properties": {
        "google": {
            "hostedDomain": "my-custom-domain.com"
        }
      },   
      "permissions": {
           "google": ["profile", "https://www.googleapis.com/auth/drive"]
      },
      "LocalNotifications": {
        "smallIcon": "ic_stat_icon_config_sample",
        "iconColor": "#488AFF",
        "sound": "beep.wav"
      }
    }
  }
}
